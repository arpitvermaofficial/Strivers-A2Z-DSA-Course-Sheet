// Initial Template for C++

#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
// User function Template for C++

class Solution{
public:
    vector<int> minPartition(int N)
    {
        // code here
        vector<int>v;
        while(N){
            if(N>=2000){
                for(int i=0;i<N/2000;i++){
                v.push_back(2000);
                    N=N-2000;
                }
            }
            else if(N>=500){
                for(int i=0;i<N/500;i++){
                v.push_back(500);
                    N=N-500;
                }
            }
            else if(N>=200){
                for(int i=0;i<N/200;i++){
                v.push_back(200);
                    N=N-200;
                }
            }
            else if(N>=100){
                for(int i=0;i<N/100;i++){
                v.push_back(100);
                    N=N-100;
                }
            }
            else if(N>=50){
                for(int i=0;i<N/50;i++){
                v.push_back(50);
                    N=N-50;
                }
            }
            else if(N>=20){
                for(int i=0;i<N/20;i++){
                v.push_back(20);
                    N=N-20;
                }
            }
            else if(N>=10){
                for(int i=0;i<N/10;i++){
                v.push_back(10);
                    N=N-10;
                }
            }
            else if(N>=5){
                for(int i=0;i<N/5;i++){
                v.push_back(5);
                    N=N-5;
                }
            }
             else if(N>=2){
                 for(int i=0;i<N/2;i++){
                v.push_back(2);
                    N=N-2;
                }
             }
            else {
                for(int i=0;i<N/1;i++){
                v.push_back(1);
                    N=N-1;
                }
            }
        }
        return v;
    }
};

//{ Driver Code Starts.

int main(){
    int t;
    cin>>t;
    while(t--){
        int N;
        cin>>N;
        
        Solution ob;
        vector<int> numbers = ob.minPartition(N);
        for(auto u: numbers)
            cout<<u<<" ";
        cout<<"\n";
    }
    return 0;
}
// } Driver Code Ends
