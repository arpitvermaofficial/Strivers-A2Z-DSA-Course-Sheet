//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
class Solution {
  public:
    bool isStraightHand(int n, int g, vector<int> &hand) {
        // code here
       if(n%g!=0)
       return false;
      map<int,int>m;
       for(int i=0;i<n;i++){
         m[hand[i]]++;
       }
       for(auto it:m){
           if(m[it.first]>0){
               for(int i=1;i<g;i++){
                   m[it.first+i]-=m[it.first];
                   if(m[it.first+i]<0)
                   return false;
               }
           }
       }
       return true;
       
    }
};

//{ Driver Code Starts.

int main() {
    int t;
    cin >> t;
    while (t--) {
        int N, groupSize;
        cin >> N >> groupSize;

        vector<int> hand(N);
        for (int i = 0; i < N; i++) cin >> hand[i];

        Solution obj;
        int ans = obj.isStraightHand(N, groupSize, hand);
        if (ans)
            cout << "True" << endl;
        else
            cout << "False" << endl;
    }
    return 0;
}
// } Driver Code Ends
